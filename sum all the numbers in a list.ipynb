   ],
   "source": [
    "print(\"Start of the Programe\".center(80,\"*\"))\n",
    "def sum(numbers):\n",
    "    total = 0\n",
    "    for x in numbers:\n",
    "        total += x\n",
    "    return total\n",
    "print(sum((8, 2, 3, 0, 7)))\n",
    "print(\"End of the Programe\".center(80,\"*\"))"
   ]
